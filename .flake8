[flake8]
# https://github.com/ambv/black/blob/master/.flake8
# E203 whitespace before ':'
# E266 Too many leading '#' for block comment
# E501 Line too long
# W503 Line break occurred before a binary operator
ignore = E203, E266, E501, W503, W504, B950
max-line-length = 120
max-complexity = 18
exclude =
    .git,
    tests,
